{
  "name": "spotify-link-extension-for-tweetdeck",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://tweetdeck.twitter.com/*"],
      "js": ["content_script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["resources/Spotify_Icon_RGB_Green.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "options_page": "options.html",
  "permissions": ["storage"]
}
